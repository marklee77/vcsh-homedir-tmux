[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/tmux.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-tmux.git tmux
           vcsh tmux remote set-url --push origin git@stillwell.me:mark/vcsh-homedir-tmux.git
           vcsh tmux remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-tmux.git
           vcsh tmux remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-tmux.git
