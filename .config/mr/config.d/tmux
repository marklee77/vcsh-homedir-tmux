[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/tmux.git]
checkout = vcsh clone https://github.com/marklee77/vcsh-homedir-tmux.git tmux
commit   = vcsh tmux commit -a
update   = vcsh tmux pull
push     = vcsh tmux push git@github.com:marklee77/vcsh-homedir-tmux.git 
status   = vcsh tmux status
gc       = vcsh tmux gc
